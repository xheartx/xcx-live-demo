.footer{
  display: flex;
  height: 80rpx;
  padding: 0 10rpx;
  align-items: center;
}
.footer .icon{
  width: 60rpx;
  height: 60rpx;
  margin: 0 10rpx;
}
.footer .icon image{
  width: 100%;
  height: 100%;
}
.input{
  flex: 1;
  font-size: 30rpx;
  color: #333;
  margin: 0 10rpx;
}
.submit{
  height: 60rpx;
  line-height: 60rpx;
  width: 180rpx;
  font-size: 30rpx;
  color: #fff;
  background-color: #405f80;
  border-radius: 5rpx; 
}